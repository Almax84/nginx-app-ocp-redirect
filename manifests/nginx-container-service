kind: Route
apiVersion: route.openshift.io/v1
metadata:
  name: nginx-container-service
  namespace: esercito-tekton
spec:
  host: nginx-container-service-esercito-tekton.apps.lab01.gpslab.club
  to:
    kind: Service
    name: nginx-container-service
    weight: 100
  port:
    targetPort: http
  wildcardPolicy: None
